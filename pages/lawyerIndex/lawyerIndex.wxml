<view class='laywerIndex'>
  <view style='position:fixed;width:100%;background-color:#fff;z-index:99;top:0;left:0;'>
    <!-- <view class="cu-bar bg-white  solid-bottom">
      <view class="action">
        <text class="cuIcon-title text-orange"></text> 我的订单
      </view>
    </view> -->
    <scroll-view scroll-x class="bg-white nav">
      <view class="flex text-center justify-around">
        <view class="cu-item  {{0==TabCur?'text-blue cur':''}}"  bindtap="tabSelect" data-id="{{0}}">
        未完成
        </view>
        <view class="cu-item  {{1==TabCur?'text-blue cur':''}}"  bindtap="tabSelect" data-id="{{1}}">
        已完成
        </view>
      </view>
    </scroll-view>
  </view>

  <view class='list padding'>
     <view class='item padding-xl radius  bg-white' bindtap='jumpToConsultingInfo'>
       <view class='flex justify-between  align-center'>
         <view class='text-red'><text class='cuIcon-title text-red'></text> 电话咨询</view>
         <view class='time text-gray'>2018/05/22 13:21:34</view>
       </view>
       <view class='flex justify-between align-center margin-top'>
         <view class='text-black text-xl text-bold'>家庭婚姻</view>
         <view class='text-bold'>客户：张先生</view>
       </view>
     </view>

     <view class='item padding-xl radius  bg-white margin-top'>
       <view class='flex justify-between  align-center'>
         <view class='text-blue'><text class='cuIcon-title text-blue'></text> 图文咨询</view>
         <view class='time text-gray'>2018/05/22 13:21:34</view>
       </view>
       <view class='flex justify-between align-center margin-top'>
         <view class='text-black text-xl text-bold'>家庭婚姻</view>
         <view class='text-bold'>客户：张先生</view>
       </view>
     </view>

  </view>
</view>
